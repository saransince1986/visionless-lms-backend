{
  "plugins": [
    ["transform-es2015-modules-commonjs"],
    ["transform-async-to-generator"],
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }]
  ],
  "env": {
    "production": {
      "comments": false,
      "minified": true
    }
  },
  "retainLines": true
}
