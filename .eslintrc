{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "env": {
    "jest": true,
    "jasmine": true
  },
  "rules": {
    "import/no-extraneous-dependencies": ["error", {"packageDir": "./"}],  
    "import/prefer-default-export": "off",
    "max-len": [
      2,
      120
    ],
    "prefer-arrow-callback": 0,
    "arrow-parens": [
      2,
      "always"
    ]
  }
}
